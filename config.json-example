  #---------------------------------------------------------------------------
  #--- Source xlogfiles ------------------------------------------------------
  #---------------------------------------------------------------------------

  "sources": {
    "dev" : {
      "logfile" : "xlogfile",
      "display" : "Development server",
      "dumplog" : "test_%U_%u_%s_%e"
    }
  },

  #---------------------------------------------------------------------------
  #--- Tournament time span --------------------------------------------------
  #---------------------------------------------------------------------------

  # Only games starting on the "starttime" or later or ending before "endtime"
  # are included in the processing. The times must be in unix epoch format (
  # ie. number of seconds since 1970-01-01 00:00)

  "time" : {
    "starttime" : 1541026800,
    "endtime"   : 1543618800
  },

  #---------------------------------------------------------------------------
  #--- Scum game qualifiers --------------------------------------------------
  #---------------------------------------------------------------------------

  # 'minturns' is minimum number of turns that the game needs to last for it
  # to be considered non-scummed
  #---
  # 'minquitturns' is the minimum number of turns that the game needs to last
  # to be considered non-scummed if it is either quit or escaped

  "scum" : {
    # "minturns" : 10,
    "minquitturns": 100
  },

  #---------------------------------------------------------------------------
  #-- Templates configuration ------------------------------------------------
  #---------------------------------------------------------------------------

  "templates"  : {
    "include"  : "templates/include",
    "path"     : "templates",
    "html"     : "www"
  },

